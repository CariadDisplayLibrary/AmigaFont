AmigaFont *diskFont = NULL;
uint8_t *diskFontData = NULL;

void setup()
{
    FRESULT fr;
    DFILE f;
    
	tft.initializeDevice();
    tft.setBacklight(255);
	tft.setTextWrap(false);
	tft.setRotation(1);
	tft.fillScreen(Color::Black);
    tft.setFont(ROMFont);

    tft.setTextColor(Color::White, Color::White);
    
    tft.print("Initializing SD card...");
    
    fr = DFATFS::fsmount(dSDVol, DFATFS::szFatFsVols[0], 1);
    
    if (fr != FR_OK) {
        tft.setTextColor(Color::Red, Color::Red);
        tft.println("Failed");
        while(1);
    }
    tft.setTextColor(Color::Green, Color::Green);
    tft.println("Done");

    tft.setTextColor(Color::White, Color::White);

    tft.print("Opening font...");
    fr = f.fsopen(FONT_FILE, FA_READ);
    if (fr != FR_OK) {
        tft.setTextColor(Color::Red, Color::Red);
        tft.println("Failed");
        while(1);
    }
    tft.setTextColor(Color::Green, Color::Green);
    tft.println("Done");

    tft.setTextColor(Color::White, Color::White);

    uint32_t s = f.fssize();
    tft.printf("Allocating %d bytes...", s);
    diskFontData = (uint8_t *)malloc(s);
    if (!diskFontData) {
        tft.setTextColor(Color::Red, Color::Red);
        tft.println("Failed");
        while(1);
    }
    tft.setTextColor(Color::Green, Color::Green);
    tft.println("Done");

    tft.setTextColor(Color::White, Color::White);

    tft.print("Reading font data...");

    f.fslseek(0);

    uint32_t r;
    uint32_t toRead = s;
    uint8_t *ptr = diskFontData;

    while (toRead > 0) {    
        fr = f.fsread(ptr, 512, &r);
        if (fr != FR_OK) {
            tft.setTextColor(Color::Red, Color::Red);
            tft.println("Failed");
            while(1);
        }
        ptr += r;
        toRead -= r;
    }
    tft.setTextColor(Color::Green, Color::Green);
    tft.printf("%d bytes read\r\n", ptr - diskFontData);
    f.fsclose();


    diskFont = new AmigaFont(diskFontData);

    tft.setFont(diskFont);

    tft.setTextColor(Color::White, Color::White);

    tft.println();
    tft.println(diskFont->getName());
    tft.printf("Font height: %d\r\n", tft.stringHeight(","));
    tft.printf("String width (\"ABC\"): %d\r\n", tft.stringWidth("ABC"));  

    tft.setTextColor(Color::Red, Color::Black);
}

void loop()
{

    tft.setCursor(50, 180);
    tft.printf("Hello World %d   ", millis());
}
